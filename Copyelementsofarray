#include <stdio.h>

int main() {
    int a[100], b[100], n;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter elements of array A:\n");
    for(int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    // Copy elements
    for(int i = 0; i < n; i++) {
        b[i] = a[i];
    }

    printf("Elements of array B:\n");
    for(int i = 0; i < n; i++) {
        printf("%d ", b[i]);
    }

    return 0;
}

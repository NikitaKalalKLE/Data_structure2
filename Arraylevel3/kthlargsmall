#include <stdio.h>

int main() {
    int arr[100], n, k, i, j, temp;

    // Input size
    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Input array
    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Input k
    printf("Enter value of k: ");
    scanf("%d", &k);

    // Sort array (Selection Sort)
    for (i = 0; i < n - 1; i++) {
        for (j = i + 1; j < n; j++) {
            if (arr[i] > arr[j]) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    if (k > 0 && k <= n) {
        printf("Kth Smallest Element = %d\n", arr[k - 1]);
        printf("Kth Largest Element  = %d\n", arr[n - k]);
    } else {
        printf("Invalid value of k\n");
    }

    return 0;
}
